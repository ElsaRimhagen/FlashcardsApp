{
  "expo": {
    "name": "FlashcardsApp",
    "slug": "FlashcardsApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/applogo_v3.png",
    "splash": {
      "image": "./assets/icons/splash_cat.png",
      "resizeMode": "contain",
      "backgroundColor": "#4C97FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.BlockisCode",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/applogo_v3.png",
        "backgroundColor": "#4C97FF"
      }
    },
    "web": {
      "favicon": "./assets/icons/applogo_v3.png"
    }
  }
}

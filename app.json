{
  "expo": {
    "name": "BlockisCode",
    "slug": "BlockisCode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/applogo.png",
    "splash": {
      "image": "./assets/icons/splash_cat.png",
      "resizeMode": "contain",
      "backgroundColor": "#4C97FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package" : "com.BlockisCode",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/applogo.png",
        "backgroundColor": "#4C97FF"
      }
    },
    "web": {
      "favicon": "./assets/icons/applogo.png"
    }
  }
}
